{
  "analytics": {
    "enable": true
  },
  "speedInsights": {
    "enable": true
  },
  "functions": {
    "app/api/**.js": {
      "maxDuration": 30
    }
  },
  "regions": ["hnd1", "sfo1", "iad1"],
  "framework": "vite",
